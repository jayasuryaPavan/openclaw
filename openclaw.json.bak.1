{
  "$include": "secrets.json",
  "models": {
    "providers": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com",
        "api": "openai-completions",
        "models": [
          {
            "id": "deepseek-reasoner",
            "name": "DeepSeek R1",
            "reasoning": true,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-chat",
            "name": "DeepSeek V3",
            "reasoning": false,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 64000,
            "maxTokens": 8192
          }
        ]
      },
      "google": {
        "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
        "api": "google-generative-ai",
        "models": [
          {
            "id": "gemini-1.5-flash",
            "name": "Gemini 1.5 Flash",
            "input": [
              "text",
              "image",
              "audio"
            ],
            "contextWindow": 1000000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "google-antigravity/gemini-3-flash"
      },
      "workspace": "c:/Work Space/Panda Chat/openclaw",
      "imageModel": {
        "primary": "google/gemini-1.5-flash"
      },
      "maxConcurrent": 4,
      "thinkingDefault": "off",
      "subagents": {
        "maxConcurrent": 8,
        "model": "deepseek/deepseek-reasoner"
      }
    },
    "list": [
      {
        "id": "panda",
        "name": "Panda Chat",
        "model": {
          "primary": "deepseek/deepseek-chat",
          "fallbacks": [
            "deepseek/deepseek-reasoner",
            "google/gemini-1.5-flash"
          ]
        },
        "subagents": {
          "model": "deepseek/deepseek-reasoner"
        }
      }
    ]
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "streamMode": "off",
      "dmSystemPrompt": "CRITICAL: Respond ONLY in casual Telugu written in English script (Tenglish). Call the user 'pandu'. Keep under 10 words. No headers. No English replies. Occasional emoji at end allowed."
    }
  },
  "gateway": {
    "mode": "local",
    "tailscale": {
      "mode": "off"
    },
    "auth": {
      "mode": "token"
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "auth": {
    "profiles": {
      "google-antigravity:jayasuryadeveloper09@gmail.com": {
        "provider": "google-antigravity",
        "mode": "oauth"
      }
    }
  },
  "discovery": {
    "wideArea": {
      "enabled": false
    }
  },
  "tools": {
    "profile": "full",
    "exec": {
      "host": "gateway",
      "security": "allowlist",
      "ask": "on-miss",
      "pathPrepend": [
        "C:\\Python314",
        "C:\\Python314\\Scripts"
      ]
    },
    "alsoAllow": [
      "agency",
      "1password",
      "desktop",
      "python",
      "python.exe",
      "node",
      "npm",
      "git",
      "echo",
      "dir",
      "ls"
    ],
    "media": {
      "audio": {
        "enabled": true,
        "models": [
          {
            "provider": "google",
            "model": "gemini-1.5-flash",
            "capabilities": [
              "audio"
            ]
          }
        ]
      }
    }
  },
  "skills": {
    "allowBundled": [
      "agency",
      "desktop",
      "1password",
      "google-search"
    ]
  },
  "browser": {
    "enabled": true,
    "headless": false,
    "executablePath": "C:/Program Files/BraveSoftware/Brave-Browser/Application/brave.exe",
    "profiles": {
      "brave": {
        "cdpPort": 9222,
        "color": "#FF4500"
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions",
    "tts": {
      "auto": "inbound",
      "provider": "edge",
      "edge": {
        "voice": "en-US-ChristopherNeural"
      }
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      },
      "google-oauth": {
        "enabled": false
      },
      "google-antigravity-auth": {
        "enabled": true
      },
      "memory-core": {
        "enabled": true
      }
    }
  },
  "meta": {
    "lastTouchedVersion": "2026.2.6-3",
    "lastTouchedAt": "2026-02-09T05:33:46.133Z"
  }
}